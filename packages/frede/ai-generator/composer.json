{
    "name": "frede/ai-generator",
    "description": "Um gerador de CRUD, refatoração e remoção para Laravel, movido por IA.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Frederyco Miguel Muniz Belém",
            "email": "frederycomiguel@gmail.com"
        }
    ],
    "require": {
